<p id="notice"><%= notice %></p>

<h1>Comment Tags</h1><br />

<div class="panel panel-default">

  <table class="table">
    <thead>
      <tr>
        <th style="padding:10px">Comment Tag</th>
      </tr>
    </thead>

    <tbody>
      <% @issues.each do |issue| %>
        <tr>
          <td style="padding:10px"><%= issue.issue_tag %></td>
        </tr>
      <% end %>
    </tbody>
  </table>

</div>
